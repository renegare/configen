{
    "name": "renegare/configen",
    "description": "Helpful tool to render a file containing mustache tags using environment variables as data (e.g configuration template)",
    "authors": [{
        "name": "Mudi Ugbowanko",
        "email": "mudi@renegare.com"
    }],

    "autoload": {
        "psr-4": {
            "Renegare\\Configen\\": "src",
            "Renegare\\Configen\\Test\\": "test"
        }
    },

    "require": {
        "php": ">=5.4.0",
        "renegare/will": "dev-master",
        "symfony/console": "2.5.6",
        "mustache/mustache": "2.7.0"
    },

    "require-dev": {
        "phpunit/phpunit": "4.3.5",
        "mikey179/vfsStream": "1.4.0"
    },

    "bin": ["configen"]
}
